; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Camera",
"class": "ShakeCamera",
"language": "GDScript",
"path": "res://addons/camera_shake/camera_scripts/shake_camera.gd"
}, {
"base": "Camera2D",
"class": "ShakeCamera2D",
"language": "GDScript",
"path": "res://addons/camera_shake/camera_scripts/shake_camera_2D.gd"
}, {
"base": "Resource",
"class": "tower",
"language": "GDScript",
"path": "res://scripts/tower_resource.gd"
} ]
_global_script_class_icons={
"ShakeCamera": "",
"ShakeCamera2D": "",
"tower": ""
}

[application]

config/name="Towerdef"
run/main_scene="res://scenes/main_menu.tscn"
config/icon="res://art/icon.png"

[display]

window/size/height=640
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/camera_shake/plugin.cfg", "res://addons/radial_menu/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
